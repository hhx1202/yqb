.container .item{
  position:relative;
  width: 100%;
  height: 180rpx;
  padding: 10rpx;
  margin-bottom: 10rpx;
  border-top: 1px solid #e4e4e4;
  border-bottom: 1px solid #e4e4e4;
  background-color: #FFFFFF;
}
.container .item .title{
  margin: 5px 10px;
  overflow: hidden;
  display: block;
  color: #333;
  font-size: 20px;
  height: 80rpx;
}

.container .item .time{
  margin-right: 60rpx;
  color: #939393;
  float: right;
  height: 70rpx;
}
.container .item .type{
  margin-left: 20rpx;
  color: #939393;
  float: left;
  height: 70rpx;
}

.arrow{
  position:absolute;
  top: 70rpx;
  right: 20rpx;
  width: 24px;
  height: 24px;
}
.arrow image{ 
  width:24px; height: 24px;
}
